<?xml version="1.0"?>
<odoo>
    <record id="view_inventory_adjustment_for_basb_info" model="ir.ui.view">
        <field name="name">view.stock.inventory.basb.info</field>
        <field name="model">stock.inventory</field>
        <field name="inherit_id" ref="stock.view_inventory_form" />
        <field name="arch" type="xml">
            <data>
                <xpath expr="//field[@name='date']" position="after">
                    <field name="meta_year"/>
                    <field name="meta_quarter"/>
                </xpath>

            </data>
        </field>
    </record>

    
</odoo>